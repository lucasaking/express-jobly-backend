{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.company.com/jobsNew.schema.json",
  "type": "object",
  "properties": {
    "companyHandle": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "salary": {
      "type": "integer",
      "minimum": 0
    },
    "equity": {
      "type": "number",
      "maximum": 1.0
    }
  },
  "additionalProperties": false,
  "required": [
    "companyHandle",
    "title"
  ]
}